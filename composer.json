{
    "name": "eyika/atom",
    "description": "Framework template for Atom, An MVC microframework for PHP. Quite heavily inspired by Laravel",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "../atomframework",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "eyika/atom-framework": "dev-main",
        "nesbot/carbon": "^3.7",
        "monolog/monolog": "^3.7",
        "predis/predis": "^2.2",
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^6.9",
        "robmorgan/phinx": "^0.16.2",
        "guzzlehttp/guzzle": "^7.9",
        "dragonmantank/cron-expression": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
    "license": "MIT",
    "scripts": {
        "post-create-project-cmd": [
            "Eyika\\Atom\\Framework\\Support\\Setup::postCreateProject",
            "cp .env.example .env"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "Abdulbasit",
            "email": "basttyy@gmail.com"
        }
    ],
    "minimum-stability": "beta"
}
